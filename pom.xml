			<version>${poi.version}</version>
		</dependency>
		<dependency>
			<groupId>com.jgoodies</groupId>
			<artifactId>jgoodies-forms</artifactId>
			<version>${forms.version}</version>
		</dependency>
		<dependency>
			<groupId>ConfigManagerProxy</groupId>
			<artifactId>ConfigManagerProxy</artifactId>
			<version>1.0.0</version>
			<scope>system</scope>
			<systemPath>${wmb.configmanagerproxy.path}/ConfigManagerProxy.jar</systemPath>
		</dependency>
